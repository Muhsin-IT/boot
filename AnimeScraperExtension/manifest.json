{
  "manifest_version": 3,
  "name": "Samehadaku Anime Scraper",
  "version": "1.0",
  "description": "Mengambil detail anime dan link episode dari Samehadaku.",
  "permissions": ["activeTab","scripting" , "storage"], 
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://v1.samehadaku.how/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png"
  }
}
